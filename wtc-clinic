CDIR=`readlink -f .`

case "$1" in

patient)
    case "$2" in 
    view)
        python3 $CDIR/functions/patients/view_patient_slots.py $3
        ;;

    slots)
        python3 $CDIR/functions/patients/view_slots_for_patient_signup.py $3
        ;;

    signup)
        python3 $CDIR/functions/patients/signup_to_patient_slot.py $3 $4 $5
        ;;

    delete)
        python3 $CDIR/functions/patients/delete_patient_slot.py $3 $4
        ;;
    esac
    ;;

clinician)
    case "$2" in
    view)
        python3 $CDIR/functions/clinicians/view_clinician_slots.py $3
        ;;

    create)
        python3 $CDIR/functions/clinicians/create_clinician_slot.py $3 $4 $5 $6
        ;;

    delete)
        python3 $CDIR/functions/clinicians/delete_clinician_slot.py $3 $4
        ;;
    esac
    ;;
esac
    
